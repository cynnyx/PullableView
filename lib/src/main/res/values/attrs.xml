<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullableView">
        <attr name="anchorOffsetUp" format="integer"/>
        <attr name="anchorOffsetDown" format="integer"/>
        <attr name="scrollThresholdUp" format="integer"/>
        <attr name="scrollThresholdDown" format="integer"/>
        <attr name="direction" format="enum">
            <enum name="UP" value="0"/>
            <enum name="DOWN" value="1"/>
            <enum name="BOTH" value="2"/>
        </attr>
    </declare-styleable>
</resources>